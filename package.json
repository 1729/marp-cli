{
  "name": "@marp-team/marp-cli",
  "version": "0.0.0",
  "description": "A CLI interface for Marp and Marpit based converters",
  "repository": "https://github.com/marp-team/marp-cli",
  "author": {
    "name": "Marp team",
    "url": "https://github.com/marp-team"
  },
  "contributors": [
    {
      "name": "Yuki Hattori",
      "url": "https://github.com/yhatt"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "marp": "./marp-cli.js"
  },
  "scripts": {
    "build": "yarn --silent clean && rollup -c",
    "check-ts": "tsc --noEmit",
    "clean": "rimraf lib",
    "format": "prettier \"**/*.{css,js,json,md,scss,ts,yaml,yml}\"",
    "format:check": "yarn --silent format -l",
    "format:write": "yarn --silent format --write",
    "lint:ts": "tslint \"{src,test}/**/*.ts\"",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "test": "jest",
    "watch": "rollup -w -c"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/pug": "^2.0.4",
    "@types/yargs": "^11.1.1",
    "autoprefixer": "^9.1.1",
    "codecov": "^3.0.4",
    "cssnano": "^4.0.5",
    "jest": "^23.5.0",
    "jest-junit": "^5.1.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "pug": "^2.0.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-pug": "^0.1.6",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^4.0.0",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "@marp-team/marp-core": "^0.0.2",
    "@marp-team/marpit": "^0.0.12",
    "chalk": "^2.4.1",
    "globby": "^8.0.1",
    "yargs": "^12.0.1"
  }
}
